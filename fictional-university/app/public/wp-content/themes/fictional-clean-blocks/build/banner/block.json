{
  "$schema": "http://json-schema.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ourblocktheme/banner",
  "title": "Fictional Banner",
  "supports": {
    "align": [
      "full"
    ]
  },
  "attributes": {
    "align": {
      "type": "string",
      "default": "full"
    },
    "imageID": {
      "type": "number"
    },
    "imageURL": {
      "type": "string",
      "default": "banner.fallbackimage"
    }
  },
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}