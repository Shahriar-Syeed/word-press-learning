{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "styles":{
    "blocks": {
      "core/button":{
        "color":{
          "text":"#ffffff",
          "background":"var(--wp--preset--color--primary)"
        }
      }
    }
  },
  "settings": {
    "typography": {
      "defaultFontSizes": false,
      "fontSizes": [],
      "customFontSize": false
    },
    "blocks": {
      "core/button":{
        "border": {
          "color": true,
          "radius": true,
          "style": true,
          "width": true
        }
      }
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "800px"
    },
    "color": {
      "palette": [
        {
          "slug": "primary",
          "color": "#0d3b66",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "#ee964b",
          "name": "Secondary"
        },
        {
          "slug": "background",
          "color": "#ffffff",
          "name": "Background"
        },
        {
          "slug": "foreground",
          "color": "#333333",
          "name": "Foreground"
        }
      ]
    }
  },
  "customTemplates": [
    {
      "name": "emptycanvas",
      "postTypes": ["page"],
      "title": "Empty Canvas"
    }
  ]
}